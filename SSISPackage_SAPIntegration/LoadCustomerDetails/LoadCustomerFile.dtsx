		public void Main()
		{
			// TODO: Add your code here

			Dts.TaskResult = (int)ScriptResults.Success;
            string InboundFolder = Dts.Variables["User::InboundFolder"].Value.ToString();
            string[] fileArray = Directory.GetFiles(InboundFolder);
           
            List<string> lstFileName = new List<string>();

            foreach (string item in fileArray)
            {
                if (item.ToString().Contains("CUSTOMERMASTER_TO_SEMP_CL_"))
                {
                    lstFileName.Add(item.ToString().Substring(28));
                }
            }

            if (lstFileName.Count == 0)
            {
                Dts.Variables["User::CustomerFileName"].Value = "";
                Dts.Variables["User::NoFileError"].Value = true;
                Dts.Variables["User::DisableTask"].Value = true;
            }
            else if (lstFileName.Count == 1)
            {
                Dts.Variables["User::NoFileError"].Value = false;
                Dts.Variables["User::DisableTask"].Value = false;
                Dts.Variables["User::CustomerFileName"].Value = lstFileName[0].ToString();
            }
            //else if (lstFileName.Count > 1)
            //{

            //}
            
		}

